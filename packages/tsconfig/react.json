{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React",
  "extends": "./base.json",
  "compilerOptions": {
    "checkJs": true,

    "lib": ["DOM", "DOM.Iterable", "ES2023"],
    "jsx": "react-jsx"
  },
  "include": [
    "${configDir}/**/*.ts",
    "${configDir}/**/*.tsx",
    "${configDir}/.storybook/*.js",
    "${configDir}/.storybook/*.ts"
  ]
}
