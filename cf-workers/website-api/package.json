{
  "name": "@arishi/website-api",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "exports": {
    ".": "./src/package-export.ts"
  },
  "scripts": {
    "build": "wrangler deploy --dry-run --minify --outdir dist",
    "deploy": "wrangler deploy --minify",
    "dev": "wrangler dev",
    "start": "wrangler dev",
    "lint": "eslint . && prettier --check .",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "typegen": "wrangler types"
  },
  "devDependencies": {
    "@arishi/eslint": "workspace:*",
    "@arishi/prettier-config": "workspace:*",
    "@arishi/tsconfig": "workspace:*",
    "@cloudflare/vitest-pool-workers": "0.6.16",
    "@cloudflare/workers-types": "^4.20250204.0",
    "typescript": "^5.7.3",
    "vitest": "2.1.9",
    "wrangler": "^3.109.1"
  },
  "packageManager": "pnpm@9.15.5",
  "dependencies": {
    "hono": "^4.7.1"
  }
}
