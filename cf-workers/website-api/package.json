{
  "name": "@arishi/website-api",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "exports": {
    ".": "./src/package-export.ts"
  },
  "scripts": {
    "build": "wrangler deploy --dry-run --minify --outdir dist",
    "deploy": "wrangler deploy --minify",
    "dev": "wrangler dev",
    "start": "wrangler dev",
    "lint": "eslint . && prettier --check .",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "typegen": "wrangler types"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "0.5.13",
    "@cloudflare/workers-types": "^4.20240925.0",
    "@arishi/eslint": "workspace:*",
    "@arishi/prettier-config": "workspace:*",
    "@arishi/tsconfig": "workspace:*",
    "typescript": "^5.6.2",
    "vitest": "2.1.2",
    "wrangler": "^3.79.0"
  },
  "packageManager": "pnpm@9.12.0"
}
